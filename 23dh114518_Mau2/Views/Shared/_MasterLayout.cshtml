<style type="text/css">
    html
    {
        font-size:14px;
        font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
    }
    body
    {
        background-color:wheat;
    }
    # container
    {
        border:2px solid #ff0000;
        border-radius:10px;
    }
    header
    {
        background-color:burlywood;
        border:4px solid #000000;
        padding:4px;
        display:flex;
        justify-content: space-between;
        align-items:center;
    }
    .logo img
    {
        height:50px;
    }
    nav ul
    {
        list-style-type:none;
        display:flex;
    }
    nav ul li
    {
        margin-left:50px;
    }
    nav ul li a
    {
        text-decoration:none;
        color: #000000;
        font-weight:bold;
    }
</style>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div id=" container">
        <header>
            <div class="logo">
                <img src="data:image/jpeg;base64," alt="logo"
            </div>
            <nav>
                <ul>
                    <li><a href="~/Home/Test">Trang chủ</a></li>
                    <li><a href="~/Product/ProductDetail">Sản phẩm</a></li>
                    <li><a href="~/Product/ShoppingCart">Giỏ hàng</a></li>
                    <li><a href="~/Home/Contact">Liên hệ</a></li>

                </ul>
            </nav>
        </header>
        <section>
            <div id=" banner"></div>
            <div>
                @RenderBody()
            </div>
        </section>
        <footer>
            FOOTER
        </footer>
    </div>
</body>
</html>
